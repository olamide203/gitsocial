{
  "name": "gittz",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:server": "yarn workspace @gittz/backend dev",
    "dev:client": "yarn workspace @gittz/client dev",
    "build:client": "yarn workspace @gittz/client build & yarn workspace @gittz/client postbuild",
    "build:css": "yarn workspace @gittz/client build:css",
    "dev:css": "yarn workspace @gittz/client dev:css",
    "start": "yarn workspace @gittz/backend start",
    "dev": "yarn run dev:server & yarn run dev:client & yarn dev:css"
  },
  "dependencies": {
    "react-github-calender": "^1.1.0"
  }
}
